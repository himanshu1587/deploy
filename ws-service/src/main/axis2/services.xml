<?xml version="1.0" encoding="UTF-8"?>
<service name="DeployService" scope="application" class="org.intalio.deploy.deployment.ws.DeployWSLifeCycle">
    <description>Deployment Service</description>
    <operation name="deployAssembly">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <operation name="undeployAssembly">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <operation name="getDeployedAssemblies">
        <messageReceiver class="org.apache.axis2.receivers.RawXMLINOutMessageReceiver"/>
    </operation>
    <parameter name="ServiceClass">org.intalio.deploy.deployment.ws.DeployWS</parameter>
</service>

